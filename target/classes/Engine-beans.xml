<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="gasolineEngine" class="com.enigma.bean.Gasoline"></bean>
    <bean id="electricalEngine" class="com.enigma.bean.Electric">
        <property name="electricName"><value>Battery</value>
        </property>
    </bean>
    <bean id="dieselEngine" class="com.enigma.bean.Diesel"></bean>
</beans>